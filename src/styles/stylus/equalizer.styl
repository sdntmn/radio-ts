.box 
  position: absolute
  top: 5px
  right: 5px
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  width: 14px;
  height: 14px;

@keyframes go-up-down 
  0% { height: 0; }
  100% { height: 14px; }


.line-1 {
  position: relative;
  width: 3px;
  height: 14px;
  background-color: rgb(252, 202, 37);;
  animation: go-up-down 0.7s infinite alternate;
}

.line-2 {
  position: relative;
  width: 3px;
  height: 10px;
  background-color: rgb(252, 202, 37);
  animation: go-up-down 0.6s infinite alternate;
}

.line-3 {
  position: relative;
  width: 3px;
  height: 8px;
  background-color: rgb(252, 202, 37);
  animation: go-up-down 0.5s infinite alternate;
}

